{
  "name": "ganevru",
  "version": "1.0.0",
  "description": "ganev.ru site",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ganevru/ganevru-gatsby.git"
  },
  "license": "MIT",
  "author": "Ivan Ganev <iganevru@gmail.com>",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "precommit": "lint-staged",
    "dev": "gatsby develop",
    "develop": "gatsby develop",
    "lint": "npm run lint:ts && npm run lint:js",
    "lint-staged": "lint-staged",
    "lint:js": "tslint \"./src/**/*.js\" \"./src/**/*.jsx\"",
    "lint:ts": "tslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "@types/lodash": "^4.14.120",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.6",
    "gatsby": "^2.0.91",
    "gatsby-cli": "^2.4.8",
    "gatsby-plugin-google-tagmanager": "^2.0.7",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-plugin-typescript": "^2.0.3",
    "gatsby-source-filesystem": "^2.0.16",
    "grommet": "^2.3.1",
    "grommet-icons": "^4.1.0",
    "lodash": "^4.17.11",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3",
    "ts-loader": "^5.3.3",
    "tsutils": "^3.7.0",
    "typescript": "^3.2.4",
    "terser": "3.14.0",
    "snyk": "^1.189.0"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0"
  },
  "engines": {
    "node": ">=8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && gatsby build"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "snyk": true
}
